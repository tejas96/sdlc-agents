{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "dist/**", "build/**"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "lint": {
        "outputs": []
      },
      "type-check": {
        "outputs": []
      },
      "test": {
        "outputs": [],
        "dependsOn": []
      }
    }
  }
